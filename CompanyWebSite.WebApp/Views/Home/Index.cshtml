            <div class="row justify-content-between">
                <div class="col-xxl-5 col-xl-5 mb-5 mb-xl-0">
                    <div class="sticky-top" style="top: 80px;">
                        <div class="section-title">
                            @foreach (var item in Model)
                            {
                                foreach (var service in item.Services)
                                {
                                    var firstPage = service.Pages.FirstOrDefault(x => x.Id == 9);
                                    if (firstPage != null)
                                    {
                                        <span class="sub-title">
                                            <img class="img-fluid" src="images/subtitle-icon.png" alt=""> @firstPage.PageContentTitle
                                        </span>
                                        <h2 class="title"> @firstPage.PageMainSlogan</h2>
                                        <div class="ps-xxl-5 ms-0 ms-md-5">
                                            <p>@firstPage.PageSubSlogan</p>
                                        </div>
                                    }
                                }
                            }
                        </div>
                    </div>
                </div>
                <div class="col-xxl-6 col-xl-7">
                    <div class="row">
                        @for (int i = 0; i < 2; i++)
                        {
                            
                            <div class="col-sm-6">
                                @foreach (var item in Model)
                                {
                                    foreach (var serviceItem in item.Services)
                                    {
                                        <div class="services grid-wrapper">
                                            <div class="service-wrapper service-style-1">
                                                <div class="service-inner">
                                                    @foreach (var media in serviceItem.ServiceMediaItems)
                                                    {
                                                        if (media.MediaType == "image")
                                                        {
                                                            <div class="service-icon">
                                                                <img class="img-fluid" src="@media.FilePath" alt="#">
                                                            </div>
                                                        }
                                                        else if (media.MediaType == "icon")
                                                        {
                                                            <div class="bg-icon">
                                                                <img class="img-fluid" src="images/svg/services/color-icon/Information-Security.svg" alt="#">
                                                            </div>
                                                        }
                                                    }
                                                    <div class ="service-content" >
                                                        <h5 class ="service-title" > @serviceItem.DefaultTitle</h5 >
                                                        <p > @serviceItem.Summary</p>
                                                        <div class ="service-links" >
                                                            <a class ="btn-arrow" href = "#" >
