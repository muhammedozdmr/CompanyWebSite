@model List<HomeDto>

@{
    ViewData["Title"] = "Home Page";
}

<!--=================================
Main Banner -->
<div class="main-banner main-banner-1">
    <div class="banner-inner">
        <img class="img-fluid banner-bg-one" src="images/banner/banner-01/banner-bg-01.png" alt="#">
        @foreach (var item in Model)
        {
            foreach (var home in item.CompanyInfos)
            {
                <h1 class="banner-title">@home.CompanyName</h1>
            }
        }
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <img class="img-fluid rotate rotate-img" src="images/banner/banner-01/banner-shape-01.png" alt="#">
                </div>
                <div class="col-md-6 ms-auto">
                    <div class="banner-content">
                        @foreach (var item in Model)
                        {
                            foreach (var home in item.CompanyInfos)
                            {
                                <h2>@home.CompanySlogan</h2>
                            }
                        }
                        <a class="btn btn-effect" asp-controller="Contact" asp-action="Index">
                            @foreach (var item in Model)
                            {
                                foreach (var home in item.CompanyInfos)
                                {
                                    <span>@home.HomeButtonName</span>
                                }
                            }
                            <svg width="20" height="22" viewBox="0 0 20 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <g clip-path="url(#clip0_59_253)">
                                    <path d="M19.4854 11.4293L17.0513 12.221C13.1214 13.4993 10.3036 16.9595 9.84784 21.0668C9.49371 16.981 6.71926 13.5081 2.81255 12.2604L0.210283 11.4293" stroke="white" stroke-width="2"/><path d="M9.83594 20.8889L9.83594 0" stroke="white" stroke-width="2"/>
                                </g>
                                <defs>
                                    <clipPath id="clip0_59_253">
                                        <rect width="21.3333" height="20" fill="white" transform="translate(20) rotate(90)"/>
                                    </clipPath>
                                </defs>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="content-wrapper">

<!--=================================
About Info -->
<section class="space-pt">
    <div class="container">
        <div class="row justify-content-between">

            <div class="col-lg-7 mb-5 mb-lg-0">
                <div class="section-title pb-0 pb-lg-4">
                    @foreach (var item in Model)
                    {
                        foreach (var about in item.About)
                        {
                            var firstPage = about.Pages.FirstOrDefault(x => x.Id == 1);
                            if (firstPage != null)
                            {
                                <span class="sub-title">
                                    <img class="img-fluid" src="images/subtitle-icon.png" alt=""> @firstPage.PageContentTitle
                                </span>
                                <h2 class="title"> @firstPage.PageMainSlogan</h2>
                            }
                        }
                    }
                </div>
                <div class="row">
                    <div class="col-sm-4">
                        <img class="img-fluid" src="images/about/about-03.jpg" alt="#">
                    </div>
                    <div class="col-sm-8">
                        @foreach (var item in Model)
                        {
                            foreach (var about in item.About)
                            {
                                <p class="ps-lg-3 mt-4 mt-sm-0">@about.Summary</p>
                            }
                        }
                        <div class="d-flex justify-content-center mt-5 ms-sm-5 ms-0">
                            <a class="btn btn-effect" asp-controller="About" asp-action="Index">
                                @foreach (var item in Model)
                                {
                                    foreach (var about in item.About)
                                    {
                                        <span>@about.DefaultTitle</span>
                                    }
                                }
                                <svg width="20" height="22" viewBox="0 0 20 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g clip-path="url(#clip0_59_254)">
                                        <path d="M19.4854 11.4293L17.0513 12.221C13.1214 13.4993 10.3036 16.9595 9.84784 21.0668C9.49371 16.981 6.71926 13.5081 2.81255 12.2604L0.210283 11.4293" stroke="white" stroke-width="2"/><path d="M9.83594 20.8889L9.83594 0" stroke="white" stroke-width="2"/>
                                    </g>
                                    <defs>
                                        <clipPath id="clip0_59_254">
                                            <rect width="21.3333" height="20" fill="white" transform="translate(20) rotate(90)"/>
                                        </clipPath>
                                    </defs>
                                </svg>
                            </a>
                        </div>
                    </div>
                </div>

                <div class="row mt-4 mt-sm-5">
                    <div class="col-sm-6">
                        <div class="counter counter-style-1">
                            <div class="counter-number">
                                <span class="timer mb-0" data-to="240" data-speed="2000">240</span><span class="suffix">+</span>
                            </div>
                            <div class="counter-info">
                                <span class="counter-title">Business Peoples</span>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div class="col-lg-5">
                <img class="img-fluid ps-lg-5" src="images/about/about-01.jpg" alt="#">
            </div>

        </div>
    </div>
</section>
